<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>TrocEncheres</display-name>
  <welcome-file-list>
  	<welcome-file>connexion</welcome-file>
  </welcome-file-list>
  
  <servlet>
	<servlet-name>ServletConnexion</servlet-name>
	<servlet-class>TrocEncheres.ihm.ServletConnexion</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>ServletConnexion</servlet-name>
	<url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  <servlet>
	<servlet-name>ServletInscription</servlet-name>
	<servlet-class>TrocEncheres.ihm.ServletInscription</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>ServletInscription</servlet-name>
	<url-pattern>/inscription</url-pattern>
  </servlet-mapping>
  
  
</web-app>